    <script>
    function addToWishlist(proId){
        event.preventDefault()
        $.ajax({
              url:'/add-to-wislist',
              data:{
               proId:proId
              },
              method:'POST',
              success: function (response){
               if(response.length){
               document.getElementById('badge').innerHTML=response.length
           }
           if(response.success){
            Swal.fire({
               position: 'top',
                icon: 'success',
                 title: 'Product added to wishlist',
                showConfirmButton: false,
               timer: 1500
                     })
           }else{
            Swal.fire({
               position: 'top',
                icon: 'success',
                 title: 'Already added',
                showConfirmButton: false,
               timer: 1500
                     })
           }
              }

        })

   }

   var selectedValue;

   // sort Products
          const sortSelect = document.getElementById('sort');
            sortSelect.addEventListener('change', (event) => {
             selectedValue = event.target.value;
          toAjax()
        });


        function toAjax(){
           
            $.ajax({
            url: '/shop',
            data: { 
              sort: selectedValue,
              isRender: true
            },
            method: 'get',
            success: (response) => {
              
              let html = ""
              for(let i=0;i<response.productList.length;i++){
                
                html += `
                <div class="col-lg-4 col-md-6 col-sm-6"  >
                            <div class="product__item">
                               <div class="product__item__pic set-bg id ='image-container"   data-setbg= "/imagess/${response.productList[i].image[0]}" id='product-image'>
                                
                                    <ul class="product__hover">
                                        <li><a href="" onclick="addToWishlist('${response.productList[i]._id}')"><img src="/images/icon/heart.png" alt="" > <span>Add to wishlist</span></a></li>
                                        <li><a href=""><img src="/images/icon/compare.png" alt=""> <span>Add to cart</span></a>
                                        </li>
                                        <li><a href="#"><img src="img/icon/search.png" alt=""></a></li>
                                    </ul>
                                </div>
                                <div id="mouse-overlay"></div>
                                 <div id="overlay"></div>
                                



                                <div class="product__item__text">
                                    <h6>${response.productList[i].name}</h6>
                                    <h6>${response.productList[i].discription}</h6>
                                    
                                    <div class="rating">
                                        <i class="fa fa-star-o"></i>
                                        <i class="fa fa-star-o"></i>
                                        <i class="fa fa-star-o"></i>
                                        <i class="fa fa-star-o"></i>
                                        <i class="fa fa-star-o"></i>
                                    </div>
                                    <h5>${response.productList[i].price}</h5>
                                    <a href="/product-detail?id=${response.productList._id}" class="stext-104 cl4 hov-cl1 trans-04 js-name-b2 p-b-6">
                                        ${response.productList[i].name}
                                   </a> 
                                   
                                   
                                   

                                    <div class="product__color__select">
                                        <label for="pc-4">
                                            <input type="radio" id="pc-4">
                                        </label>
                                        <label class="active black" for="pc-5">
                                            <input type="radio" id="pc-5">
                                        </label>
                                        <label class="grey" for="pc-6">
                                            <input type="radio" id="pc-6">
                                        </label>
                                    </div>
                                </div>
                            </div>
                            </div>
                        `
              }

              document.getElementById('product').innerHTML = html
            }
            })
        }
</script>

