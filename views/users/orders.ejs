<!-- Shopping Cart Section Begin -->
<section class="shopping-cart spad">
<div class="container">
 <div class="row">
     <div class="col-lg-14">
         <div class="shopping__cart__table">
            <% if(orderData.length>0){ %>
             <table>
                 <thead>
                     <tr>
                         
                        <th>Order Id</th>
                         <th>Total Amount</th>
                         <th>Payement Method</th>
                         <th>Order Status</th>
                         <th>Action</th>


                        </tr>
                 </thead>
                 <tbody>


                    <% for(let content of orderData) { %>

                     <tr>
                         

                         
                         <td class="product__cart__item">
                             <div class="product__cart__item__pic">
                             </div>
                             <div class="product__cart__item__text">
                                <a href="/ordersingle"> <h6><%= content._id %></h6></a>
                             </div>
                         </td>
                         
                         <td class="product__cart__item">
                            <div class="product__cart__item__pic">
                            </div>
                            <div class="product__cart__item__text">
                                <h6><%= content.totalPrice %></h6>
                            </div>
                        </td>
                        
                        <td class="product__cart__item">
                            <div class="product__cart__item__pic">
                            </div>
                            <div class="product__cart__item__text">
                                <h6> <%= content.paymentMethod %></h6>
                            </div>
                        </td>
                        
                         
                 
                         
                        <td class="product__cart__item">
                            <div class="product__cart__item__pic">
                            </div>
                            <div class="product__cart__item__text">
                                <h6> <%= content.status %></h6>
                            </div>
                        </td>
                        <% if(content.status !='Cancelled'){ %>
                        <td class="product__cart__item">
                            <div class="product__cart__item__pic">
                            </div>
                            <div class="product__cart__item__text">
                               <h6> <a href="/orderCancel?id=<%= content._id %>"> Cancel Order </h6> </a>
                            </div>
                        </td><% }else{ %>
                            <td class="product__cart__item">
                                <div class="product__cart__item__pic">
                                </div>
                                <div class="product__cart__item__text">
                                  <p>cancelled</p>
                                </div>
                            </td>
                                 <% } %>
                           

                       
                     </tr>
                     
                    <% } %>
                  
                 </tbody>
             </table>
             <% }else { %>
         </div>
         
     </div>
 </div>
</div>
</section>



