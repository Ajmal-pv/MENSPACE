<!-- Shopping Cart Section Begin -->

<section class="shopping-cart spad">
<div class="container">
 <div class="row">
     <div class="col-lg-14">
         <div class="shopping__cart__table">
             <table>
                 <thead>
                     <tr>
                         <th>Product</th>
                         <th>quantity</th>
                         <th>price</th>
                         <th>status</th>
                        
                        
                        
                     </tr>
                 </thead>
                 
                 <tbody>
                    <% for(let i=0;i<productData.length;i++){ %>
                     <tr>
                         
           
                         
                         
                            <td class="product__cart__item">
                                <div class="product__cart__item__pic">
                                   <img width="100px" height="100px" src="imagess/<%=productData[i].productId.image[0]%>" alt="">
                                </div>
                                <div class="product__cart__item__text">
                                    <h6><%=productData[i].productId.name%></h6>
                                  
                                   <h6>₹<%=productData[i].sellingPrice%></h6>
                                </div>
                            </td>
                             
                        
                         <td class="product__cart__item "   >
                            <p><%=productData[i].quantity%></p>
                            
                        </td>
                         
                           <td class="product__cart__item" >
                            <p> <%=productData[i].sellingPrice*productData[i].quantity%> </p>
                            </td>
                          
                            <td class="product__cart__item">
                                <p><%=orderData.status%></p>
                                
                            </td>
                           
                       
                       
                     </tr>
                     <% } %>
                       
                  
                 </tbody>
                 
             </table>
         </div>
         <div class="row">
             <div class="col-lg-6 col-md-6 col-sm-6">
                 <div class="continue__btn">
                     <a href="orders">Back to orders</a>
                 </div>
             </div>
             
         
     </div>
 </div>
</div>
</section>

 

